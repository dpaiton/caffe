net: "models/sparsenet_mnist/sparsenet.prototxt"
test_iter: 100
test_interval: 100
test_compute_loss: true
# We want to learn slowly (0.01) until bias is
# done and then speed up to 0.1
base_lr: 0.1 
lr_policy: "step"
gamma: 0.1
stepsize: 100000
display: 10
max_iter: 85000
weight_decay: 0.0001
snapshot: 1000
snapshot_prefix: "/osx/caffe/models/sparsenet_mnist/sparsenet"
momentum: 0.9
# solver mode: CPU or GPU
solver_mode: CPU
