net: "models/sparsenet/logistic/sparsenet_deep.prototxt"
test_iter: 100
test_interval: 10000
test_compute_loss: true

max_iter: 960000
base_lr: 0.02

#lr_policy: "inv"
#power: 0.75
lr_policy: "step"
stepsize: 120000

gamma: 0.5
momentum: 0.8
weight_decay: 0.001

display: 1000
snapshot: 10000
snapshot_prefix: "models/sparsenet/logistic/sparsenet_v.100.8"

display_stats: true

# solver mode: CPU or GPU
solver_mode: GPU 
