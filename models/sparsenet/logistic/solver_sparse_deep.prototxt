net: "models/sparsenet/logistic/sparsenet_deep.prototxt"
test_iter: 100
test_interval: 1000
test_compute_loss: true

max_iter: 240000
base_lr: 0.01

#lr_policy: "inv"
#power: 0.75
lr_policy: "step"
stepsize: 120000

gamma: 0.1
momentum: 0.9
weight_decay: 0.0001

display: 100
snapshot: 100000
snapshot_prefix: "models/sparsenet/logistic/sparsenet_v.90.79"

# solver mode: CPU or GPU
solver_mode: GPU 
