net: "models/sparsenet/logistic/sparsenet.prototxt"
#test_net: "models/sparsenet/logistic/mlp.prototxt" # single data layer
test_iter: 100
test_interval: 1000
test_compute_loss: true

max_iter: 1000
base_lr: 0.05

lr_policy: "step"
stepsize: 500

gamma: 0.1
momentum: 0.8
weight_decay: 0.001

display: 100
snapshot: 1000
snapshot_prefix: "models/sparsenet/logistic/sparsenet_v.test5"

# GradientStats
display_stats: true 
stats_iter: 100

# solver mode: CPU or GPU
solver_mode: CPU
