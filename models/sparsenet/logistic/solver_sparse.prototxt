net: "models/sparsenet/logistic/sparsenet.prototxt"
test_iter: 100
test_interval: 1000
test_compute_loss: true

max_iter: 3000
base_lr: 0.1 

lr_policy: "step"
stepsize: 1500

gamma: 0.1
momentum: 0.8
weight_decay: 0.0001

display: 100
snapshot: 1000
snapshot_prefix: "models/sparsenet/logistic/sparsenet_v.0.0"

# GradientStats
display_stats: true 

# solver mode: CPU or GPU
solver_mode: CPU
