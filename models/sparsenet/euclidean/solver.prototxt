net: "models/sparsenet/euclidean/sparsenet.prototxt"
test_iter: 100
test_interval: 10000
test_compute_loss: true

max_iter: 2000000
base_lr: 0.01

lr_policy: "step"
stepsize: 500000

gamma: 0.5
momentum: 0.9
weight_decay: 0.0001

display: 10000
snapshot: 100000
snapshot_prefix: "models/sparsenet/euclidean/sparsenet_v.34.0"

# solver mode: CPU or GPU
solver_mode: CPU 
