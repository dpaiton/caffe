net: "models/sparsenet/euclidean/sparsenet_deep.prototxt"
test_iter: 100
test_interval: 100000
test_compute_loss: true

max_iter: 2000000
base_lr: 0.1

lr_policy: "step"
stepsize: 75000

gamma: 0.9
momentum: 0.9
weight_decay: 0.001

display: 1000
snapshot: 100000
snapshot_prefix: "models/sparsenet/euclidean/sparsenet_v.37.0"

# solver mode: CPU or GPU
solver_mode: GPU 
