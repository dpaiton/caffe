net: "models/sparsenet/euclidean/sparsenet_deep.prototxt"
test_iter: 100
test_interval: 10000
test_compute_loss: true

max_iter: 1000000
base_lr: 0.01

lr_policy: "step"
stepsize: 250000

gamma: 0.9
momentum: 0.9
weight_decay: 0.0001

display: 1000
snapshot: 100000
snapshot_prefix: "models/sparsenet/euclidean/sparsenet_v.33.0"

# solver mode: CPU or GPU
solver_mode: GPU 
